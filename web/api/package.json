  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.12.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mqtt": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "rethinkdb": "^2.4.2",
    "rimraf": "^3.0.2",
    "socket.io": "^2.3.0"
  },
